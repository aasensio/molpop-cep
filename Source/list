gfortran -c -O3 -Wall global_molpop.f90
gfortran -c -O3 -Wall maths_molpop.f90 
gfortran -c -O3 -Wall coll_molpop.f90
gfortran -c -O3 -Wall cep_molpop_interface.f90
gfortran -c -O3 -Wall io_molpop.f90
gfortran -c -O3 -Wall maths_cep.f90	
gfortran -c -O3 -Wall constants_cep.f90
gfortran -c -O3 -Wall global_cep.f90	
gfortran -c -O3 -Wall io_cep.f90
gfortran -c -O3 -Wall functions_cep.f90
gfortran -c -O3 -Wall escape_cep.f90
gfortran -c -O3 -Wall sol_cep.f90
gfortran -c -O3 -Wall sol_molpop.f90
gfortran -c -O3 -Wall molpop.f90 
gfortran coll_molpop.o global_molpop.o io_molpop.o maths_molpop.o molpop.o\
	sol_molpop.o cep_molpop_interface.o sol_cep.o constants_cep.o\
	global_cep.o maths_cep.o escape_cep.o functions_cep.o io_cep.o\
	 -o molpop
cp molpop ../
